@{
    Layout = null;
}
<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>Tolerance Kiosk</title>
    <base href="/kiosk">
    <link href="~/lib/bootswatch-dist/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/lib/font-awesome-bower/css/font-awesome.css" rel="stylesheet" />
    <link href="~/lib/angularjs-slider/dist/rzslider.min.css" rel="stylesheet" />
    <link href="~/lib/ng-tags-input/ng-tags-input.min.css" rel="stylesheet" />
    <link href="~/lib/angular-switcher/dist/angular-switcher.min.css" rel="stylesheet" />
    <link href="~/lib/toastr/toastr.min.css" rel="stylesheet" />
    <link href="~/lib/nz-toggle/dist/nz-toggle.min.css" rel="stylesheet" />

    <link href="~/css/keyboard.css" rel="stylesheet" />
    <link href="~/css/animate.css" rel="stylesheet" />
    <link href="~/css/kiosk.css" rel="stylesheet" />
    <link href="~/css/kiosk-animations.css" rel="stylesheet" />
</head>
<body ng-app="app">

    <span us-spinner="{radius:30, width:8, length: 16}" spinner-key="spinner-1" spinner-start-active="false"></span>
    <div class="container">
        <div ui-view id="main-view" class="myanimation" spinner-start-active="true"></div>
    </div>

    <!-- Vendor Libraries -->
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootswatch-dist/js/bootstrap.min.js"></script>
    <script src="~/lib/lodash/lodash.min.js"></script>
    <script src="~/lib/moment/min/moment.min.js"></script>
    <script src="~/lib/clj-fuzzy/src-js/clj-fuzzy.js"></script>
    <script src="~/Scripts/jquery.signalR-2.2.0.js"></script>
    <script src="~/signalr/hubs"></script>
    <script src="~/lib/angular/angular.min.js"></script>
    <script src="~/lib/angular-ui-router/release/angular-ui-router.min.js"></script>
    <script src="~/lib/angular-messages/angular-messages.min.js"></script>
    <script src="~/lib/angularLocalStorage/dist/angularLocalStorage.min.js"></script>
    <script src="~/lib/angular-cookies/angular-cookies.min.js"></script>
    <script src="~/lib/angular-bootstrap/ui-bootstrap-tpls.min.js"></script>
    <script src="~/lib/angular-smart-table/dist/smart-table.min.js"></script>
    <script src="~/lib/angularjs-slider/dist/rzslider.min.js"></script>
    <script src="~/lib/ng-tags-input/ng-tags-input.min.js"></script>
    <script src="~/lib/angular-switcher/dist/angular-switcher.min.js"></script>
    <script src="~/lib/nz-toggle/dist/nz-toggle.min.js"></script>
    <script src="~/lib/toastr/toastr.min.js"></script>
    <script src="~/lib/humanize-plus/dist/humanize.min.js"></script>
    @*<script src="~/Scripts/keyboard.js"></script>*@

    <!-- Bootstrapping -->
    <script src="~/app/kiosk/app.module.js"></script>

    <!-- Reusable Blocks/Modules -->
    <script src="~/app/blocks/logger/logger.module.js"></script>
    <script src="~/app/blocks/logger/logger.js"></script>
    <script src="~/app/blocks/exception/exception.module.js"></script>
    <script src="~/app/blocks/exception/exception-handler.provider.js"></script>
    <script src="~/app/blocks/exception/exception.js"></script>
    <script src="~/app/filters/filter.module.js"></script>
    <script src="~/app/directives/smart-table.directive.js"></script>
    <script src="~/app/directives/clear-key.directive.js"></script>

    <!-- core module -->
    <script src="~/app/core/core.module.js"></script>
    <script src="~/app/core/constants.js"></script>
    <script src="~/app/core/config.js"></script>
    <script src="~/app/services/service.module.js"></script>
    <script src="~/app/filters/app.filters.js"></script>
    <script src="~/app/services/censor.service.js"></script>
    <script src="~/app/services/people.service.js"></script>
    <script src="~/app/services/user.service.js"></script>
    <script src="~/app/services/configuration.service.js"></script>

    <!-- app -->
    <script src="~/app/kiosk/kiosk.js"></script>
</body>
</html>